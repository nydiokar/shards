  "3.OCTONION:  ", show(lines[FOLD_3_OCTONION]), "L â”‚ Arrows\n",
  "4.CLIFFORD:  ", show(lines[FOLD_4_CLIFFORD]), "L â”‚ Symmetry\n",
  "5.SPINOR:    ", show(lines[FOLD_5_SPINOR]), "L â”‚ zkPerf\n",
  "6.VECTOR:    ", show(lines[FOLD_6_VECTOR]), "L â”‚ Coords\n",
  "7.TENSOR:    ", show(lines[FOLD_7_TENSOR]), "L â”‚ Compile\n",
  "8.BOTT:      ", show(lines[FOLD_8_BOTT]), "L â”‚ Period-8\n",
  "9.MONSTER:   ", show(lines[FOLD_9_MONSTER]), "L â”‚ Complete\n",
  "â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n",
